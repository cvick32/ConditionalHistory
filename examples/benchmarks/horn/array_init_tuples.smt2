(set-logic HORN)
(declare-fun inv1 ((Array Int Int) Int Int) Bool)
(declare-fun fail () Bool)
(assert (forall ((a (Array Int Int)) (N Int)) (inv1 a 0 N)))
(assert (forall ((a (Array Int Int)) (i Int) (a1 (Array Int Int)) (i1 Int) (N Int))
  (let ((a!1 (store (store (store a i (+ i 6)) (+ i 1) (+ i 5)) (+ i 2) (+ i 4))))
  (let ((a!2 (store (store (store a!1 (+ i 3) (+ i 3)) (+ i 4) (+ i 2))
                    (+ i 5)
                    (+ i 1))))
    (=> (and (inv1 a i N) (< i N) (= a!2 a1) (= i1 (+ i 6))) (inv1 a1 i1 N))))))
(assert (forall ((i Int) (N Int) (a (Array Int Int)) (i1 Int))
  (let ((a!1 (and (inv1 a i N)
                  (>= i N)
                  (<= 0 i1)
                  (< i1 N)
                  (not (>= (select a i1) 0)))))
    (=> a!1 fail))))
(assert (not fail))
(check-sat)
